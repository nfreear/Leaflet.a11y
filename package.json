{
  "name": "leaflet.a11y",
  "version": "0.2.0",
  "description": "An accessibility and localization plugin for Leaflet.",
  "main": "./Leaflet.a11y.js",
  "module": "./src/AccessibilityPlugin.mjs",
  "author": "Nick Freear",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nfreear/leaflet.a11y.git"
  },
  "peerDependencies": {
    "leaflet": "^1.0.0"
  },
  "scripts": {
    "build": "npm run build:plugin && npm run copy",
    "build:plugin": "node src/build.mjs > Leaflet.a11y.js",
    "copy": "cp Leaflet.a11y.js example/lib && cp src/* example/lib && cp -r locale example",
    "start": "npm run build && npx servor example",
    "start:debug": "npx servor",
    "fix": "semistandard --fix",
    "test": "semistandard"
  },
  "keywords": [
    "leaflet",
    "map",
    "i18n",
    "internationalization",
    "internationalisation",
    "localization",
    "translation",
    "a11y",
    "accessibility"
  ],
  "semistandard": {
    "X__include": [
      "example",
      "src"
    ]
  },
  "devDependencies": {
    "semistandard": "^17.0.0",
    "servor": "^4.0.2"
  }
}
